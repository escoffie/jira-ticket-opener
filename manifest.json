{
  "manifest_version": 3,
  "name": "Jira Ticket Opener",
  "version": "2.0",
  "description": "Open Jira tickets with history.",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "options_page": "options.html"
}